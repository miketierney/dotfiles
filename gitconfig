[alias]
  co = checkout
  pl = pull
  pu = push
  st = status -sb
  stat = status
  br = branch
  ca = commit -v -a
  ci = commit -v
  # up = !sh -c 'git pull --rebase --prune && git log --pretty=format:\"%Cred%ae %Creset- %C(yellow)%s %Creset(%ar)\" HEAD@{1}..'
  sh = !git-sh

  # Git-SVN stuff
  # sup = !sh -c 'git svn rebase && git log --pretty=format:\"%Cred%ae %Creset- %C(yellow)%s %Creset(%ar)\" HEAD@{1}'
  update = svn rebase
  up = update
  dcommit = svn dcommit
  # git log
  L = log --decorate
  l = log --pretty=oneline --decorate --abbrev-commit
  ll = log --pretty=oneline --decorate --abbrev-commit --max-count=15
  d = diff --word-diff
[push]
  default = current
[pull]
  default = current
[color]
  ui = true
  sh = auto
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
  nobranch = red bold
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "sh"]
  branch = yellow
  workdir = green
